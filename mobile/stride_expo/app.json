{
  "expo": {
    "name": "Stride Campus",
    "slug": "Stride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://media.stridecampus.com/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "https://media.stridecampus.com/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spiritbulb.stride",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["background-fetch", "remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "stridecampus.auth",
            "CFBundleURLSchemes": ["stridecampus"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://media.stridecampus.com/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.spiritbulb.stride",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "stridecampus"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#f23b36",
          "sounds": ["./assets/notification_sound.wav"],
          "mode": "production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7ad0defb-2d05-4a03-ad2b-bfcff8ea40b8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7ad0defb-2d05-4a03-ad2b-bfcff8ea40b8",
      "channel": "zone-a",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    }
  }
}